<replication>
  <message_set name="achievement info">
    <match>^ui\\00_message\\achievement\\achievement_info$</match>
    <rewrite>
      <original_match>^クエスト「\s*(.+?)\s*」クリア$</original_match>
      <translation_replace>Clear quest "{0}"</translation_replace>
      <translation_arg message_set="quest name" />
    </rewrite>
    <rewrite>
      <original_match>^サブストーリー「(.+)」のすべての使命をクリア$</original_match>
      <translation_replace>Clear all missions for substory "{0}"</translation_replace>
      <translation_arg message_set="package quest name" />
    </rewrite>
    <rewrite>
      <original_match>^ワイルドハントを(.+)回クリア$</original_match>
      <translation_replace>Clear Wild Hunt {0} times</translation_replace>
    </rewrite>
    <rewrite>
      <original_match>(.+)で&lt;ITEM (\d+)&gt;を(\d+)個発見</original_match>
      <translation_replace>Find {2} &lt;ITEM {1}&gt; in {0}</translation_replace>
      <translation_arg index="0" message_set="location name" />
    </rewrite>
    <rewrite>
      <original_match>^(.+)のジョブレベルが(\d+)になった$</original_match>
      <translation_replace>Reach job level {1} as {0:i}</translation_replace>
      <translation_arg index="0" message_set="job name" />
    </rewrite>
    <rewrite>
      <original_match>^(.+)のジョブレベルが
(\d+)に到達$</original_match>
      <translation_replace>Reach job level {1} as {0:i}</translation_replace>
      <translation_arg index="0" message_set="job name" />
    </rewrite>
    <rewrite>
      <original_match>^(.+)までの(\d+)種のジョブレベルが(\d+)に到達$</original_match>
      <translation_replace>Reach job level {2} with {1} vocations up to {0}</translation_replace>
      <translation_arg index="0" message_set="job name" />
    </rewrite>
    <rewrite>
      <original_match>^いずれかのエンブレムストーンのレベルが(\d+)に到達$</original_match>
      <translation_replace>Reach level {0} with any Emblem Stone</translation_replace>
    </rewrite>
    <rewrite>
      <original_match>^ポーン戦術育成を累計(\d+)回おこなった$</original_match>
      <translation_replace>Complete a total of {0} Pawn Tactics Training</translation_replace>
    </rewrite>
    <rewrite>
      <original_match>^マンドラゴラ生態図鑑で(\d+)種類発見$</original_match>
      <translation_replace>Discover {0} subspecies in Mandragora Observation Journal</translation_replace>
    </rewrite>
    <rewrite>
      <original_match>^黒呪の迷宮を1周踏破$</original_match>
      <translation_replace>Traverse Bitterblack Maze 1 time</translation_replace>
    </rewrite>
    <rewrite>
      <original_match>^黒呪の迷宮を(\d+)周踏破$</original_match>
      <translation_replace>Traverse Bitterblack Maze {0} times</translation_replace>
    </rewrite>
    <rewrite>
      <original_match>^《(.+)》系エネミーを(1|１)体討伐$</original_match>
      <translation_replace>Kill 1 "{0:@}" enemy</translation_replace>
      <translation_arg index="0" message_set="extra enemy name" required="true" />
    </rewrite>
    <rewrite>
      <original_match>^《(.+)》系エネミーを(.+)体討伐$</original_match>
      <translation_replace>Kill {1} "{0:@}" enemies</translation_replace>
      <translation_arg index="0" message_set="extra enemy name" required="true" />
    </rewrite>
    <rewrite>
      <original_match>^《(.+)》系エネミーを(1|１)体討伐$</original_match>
      <translation_replace>Kill 1 "{0:@}" enemy</translation_replace>
      <translation_arg index="0" message_set="npc name" />
    </rewrite>
    <rewrite>
      <original_match>^《(.+)》系エネミーを(.+)体討伐$</original_match>
      <translation_replace>Kill {1} "{0:@}" enemies</translation_replace>
      <translation_arg index="0" message_set="npc name" />
    </rewrite>
    <rewrite>
      <original_match>^(.+)を(1|１)体討伐$</original_match>
      <translation_replace>Kill {0:i}</translation_replace>
      <translation_arg index="0" message_set="npc name" />
    </rewrite>
    <rewrite>
      <original_match>^ウォーミッション「(.+)」で(.+)を(\d+)体討伐$</original_match>
      <translation_replace>Kill {2} {1:+} in War Mission "{0}"</translation_replace>
      <translation_arg index="0" message_set="quest name" />
      <translation_arg index="1" message_set="npc name" />
    </rewrite>
    <rewrite>
      <original_match>^(.+)を(\d+)体討伐$</original_match>
      <translation_replace>Kill {1} {0:+}</translation_replace>
      <translation_arg index="0" message_set="npc name" />
    </rewrite>
  </message_set>
</replication>